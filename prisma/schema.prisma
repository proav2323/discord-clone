datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}
 
model Profile {
  id String @id @default(uuid())
  userId: String @unique
  name String
  imgUrl String @db.Text
  email String @db.Text

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
} 

model Server {
  id String @id @default(uuid())
}